{
  "name": "frontend",
  "version": "0.1.0",
  "description": "The frontend application for the landing page content management system.",
  "author": {
    "name": "Konstantin MÃ¼nster",
    "email": "info@kmuenster.com"
  },
  "scripts": {
    "dev": "DISABLE_SENTRY=true next dev",
    "dev:test": "DISABLE_SENTRY=true NODE_ENV=test APP_ENV=test next dev",
    "build": "next build",
    "start": "next start -p $PORT",
    "start:test": "DISABLE_SENTRY=true NODE_ENV=test APP_ENV=test next start -p 3000",
    "lint": "eslint .",
    "check-types": "tsc --noEmit",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx|md|mdx)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm run check-types && npm run check-format && npm run lint",
    "precommit": "lint-staged",
    "test": "jest",
    "cy:run": "npx cypress run",
    "cy:open": "cypress open",
    "pretest:e2e:run": "next build",
    "test:e2e:run": "start-server-and-test start:test http://localhost:3000 cy:run",
    "test:e2e:dev": "start-server-and-test dev:test http://localhost:3000 cy:open"
  },
  "dependencies": {
    "@fontsource/overpass": "^4.5.4",
    "@sentry/nextjs": "^6.17.9",
    "core-js": "^3.21.1",
    "handlebars": "^4.7.7",
    "hex-rgb": "^5.0.0",
    "mjml": "^4.12.0",
    "next": "^12.0.10",
    "next-seo": "^4.28.1",
    "nodemailer": "^6.7.2",
    "qs": "^6.10.3",
    "react": "17.0.2",
    "react-auth-code-input": "^3.1.0",
    "react-cookie-consent": "^7.3.1",
    "react-dom": "17.0.2",
    "react-markdown": "^7.1.1",
    "react-player": "^2.9.0",
    "react-svg": "^14.1.11",
    "regenerator-runtime": "^0.13.9",
    "restyped-axios": "^2.0.0",
    "sharp": "^0.30.1",
    "slugify": "^1.6.5",
    "styled-components": "^5.3.3",
    "styled-icons": "^10.45.0",
    "styled-reset": "^4.3.4",
    "swiper": "^7.4.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@types/gtag.js": "^0.0.8",
    "@types/jest": "^27.4.0",
    "@types/mjml": "^4.7.0",
    "@types/node": "^17.0.21",
    "@types/nodemailer": "^6.4.4",
    "@types/qs": "^6.9.7",
    "@types/react": "17.0.37",
    "@types/styled-components": "^5.1.16",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.9.0",
    "babel-jest": "^27.5.1",
    "babel-plugin-styled-components": "^2.0.2",
    "cypress": "^9.5.0",
    "eslint": "^8.10.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-cypress": "^2.12.1",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.4",
    "msw": "^0.38.1",
    "prettier": "^2.5.1",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^27.1.3",
    "typescript": "4.5.2"
