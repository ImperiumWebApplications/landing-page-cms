{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "lint": {},
    "check-types": {},
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "dev": {
      "cache": false
    }
  }
}
